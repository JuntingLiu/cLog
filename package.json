{
  "name": "c-log-kit",
  "version": "0.0.1",
  "description": "Colourful console log.",
  "main": "dist/umd/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c build/rollup.config.js && pnpm dts",
